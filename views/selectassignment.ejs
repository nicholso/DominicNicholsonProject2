<% include header %>
   <form method="post" action="<%= action %>">    
   		<table id="login">
		    <tr><td>First Name:</td>
		        <!-- name attribute must be provided for non ajax forms -->
		        <td><input type="text" id="FName" name="FName" /></td>
		        <td>Last Name:</td>
		        <!-- name attribute must be provided for non ajax forms -->
		        <td><input type="text" id="LName" name="LName" /></td>
		    </tr>
		    <tr><td>Password:</td>
		        <td><input type="password" id="Password" name="Password" /></td>
		    </tr>
		</table>
    	<select id="AssignmentID" name="AssignmentID">
		    <% for (var i=0; rs.length > i; i++) { %>
		     <option value="<%= rs[i].AssignmentID %>">
		         <%= rs[i].AssignmentName %>
		     </option>
		    <% } %>
		</select>
       <input type="submit">
   </form>
   

<% include footer %>